@using Microsoft.AspNetCore.Identity
@using Apps.Data
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    int i = 0;
    ViewBag.Title = "Lista użytkowników";
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">String</th>
            <th scope="col">Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in ViewBag.claims)
        {
            i++;
            <tr>
                <th scope="row">@i.ToString()</th>
                <td>@claim</td>
                <td>
                </td>
            </tr>
        }
    </tbody>
</table>
